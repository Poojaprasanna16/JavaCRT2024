//Read a character and print character is alphanumneric or special symbol
 // If alphanumeric then print alphabet or digit
  //If alphabet then print uppercase or lower case letter


import java.util.*;
  class AlphaNumeric
{
public static void main(String args[])
{
 Scanner s=new Scanner(System.in);
 char ch=s.next().charAt(0);
 if(Character.isLetterOrDigit(ch))
{
System.out.println("Alpha-Numeric");
  if(Character.isLetter(ch))
{
System.out.println("Alphabet");
if(Character.isUpperCase(ch))
{
System.out.println("UpperCase");
}
else
{
System.out.println("Lower case");
}
}
else
{
System.out.println("Digit");
}
}
else
{
System.out.println("Special Symbol");
}
}
}


inp:R
out:Alpha-Numeric
Alphabet
UpperCase


D:\JavaCRT>java AlphaNumeric
inp:r
out:
Alpha-Numeric
Alphabet
Lower case

D:\JavaCRT>java AlphaNumeric
inp:1
Out:
Alpha-Numeric
Digit

